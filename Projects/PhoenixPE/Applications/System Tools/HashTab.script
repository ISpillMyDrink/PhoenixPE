[Main]
Title=HashTab
Description=(v6.0.0.34) HashTab is a shell extension that calculates and displays hash values from over two dozen popular hashing algorithms.
Author=Homes32
Level=5
Selected=True
Mandatory=False
Version=1.0.0.0
Date=2020-04-28
Interface=Interface

[Variables]
%ProgramFolder%="HashTab"
%SetupExe%="HashTab_v6.0.0.34_Setup.exe"
%DownloadURL%=http://implbits.com/products/hashtab/HashTab_v6.0.0.34_Setup.exe
%HashTabVer%=6.0.0.34

[Process]
Echo,"Processing %ScriptTitle%..."

RunFromRam

///////////////////////////////////////////////////////////////////////////////////
// Extract

If,Not,ExistFile,"%ProgramsCache%\%ProgramFolder%\%SetupExe%",Run,%ScriptFile%,DownloadProgram
DirDeleteEx,"%TargetPrograms%\%ProgramFolder%"
ShellExecute,Hide,"%Tools%\7z.exe","x -y #$q%ProgramsCache%\%ProgramFolder%\%SetupExe%#$q -xr!$PLUGINSDIR -xr!uninst.exe.nsis -o#$q%TargetPrograms%\%ProgramFolder%#$q"
If,Not,#r,Equal,0,Halt,"Extract [%SetupExe%] failed: The code returned was [#r]."
If,%SourceArch%,Equal,x64,FileRename,"%TargetPrograms%\%ProgramFolder%\$R0","%TargetPrograms%\%ProgramFolder%\HashTab64-%HashTabVer%.dll"
Else,FileDelete,"%TargetPrograms%\%ProgramFolder%\$R0"

///////////////////////////////////////////////////////////////////////////////////
// Register Ext.

Run,%ScriptFile%,RegisterShellExtension

///////////////////////////////////////////////////////////////////////////////////
// Settings

RegHiveLoad,Tmp_Default,%RegDefault%
If,%cb_LowercaseHashes%,Equal,True,RegWrite,HKLM,0x4,"Tmp_Default\Software\HashTab\Settings","UseLowercase",1
