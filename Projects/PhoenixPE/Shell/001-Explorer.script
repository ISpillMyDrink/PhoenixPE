[Main]
Title=Explorer Shell
Description=Explorer Shell and File Explorer for Windows 10.
Author=Homes32
Level=3
Selected=True
Mandatory=False
Version=1.2.1.0
Date=2020-07-09

[TODO]
// Catalog Files

[Variables]

[Process]

///////////////////////////////////////////////////////////////////////////////////
// Explorer Files
Run,%ScriptFile%,ExtractFiles

///////////////////////////////////////////////////////////////////////////////////
// Config
Echo,"Preparing Registry Hives..."
RegHiveLoad,Tmp_Software,%RegSoftware%
RegHiveLoad,Tmp_Default,%RegDefault%
RegHiveLoad,Tmp_Drivers,%RegDrivers%
RegHiveLoad,Tmp_System,%RegSystem%
RegHiveLoad,Tmp_Install_Software,%RegInstallSoftware%
RegHiveLoad,Tmp_Install_System,%RegInstallSystem%

ExtractAllFiles,%ScriptFile%,Reg,%ProjectTemp%

Run,%ScriptFile%,Reg-Explorer
Run,%ScriptFile%,Reg-Themes
Run,%ScriptFile%,Reg-BitLocker
Run,%ScriptFile%,Reg-MMC
Run,%ScriptFile%,Reg-Search
Run,%ScriptFile%,Reg-Options

RegHiveUnload,Tmp_Software
RegHiveUnload,Tmp_Default
RegHiveUnload,Tmp_Drivers
RegHiveUnload,Tmp_Install_Software
RegHiveUnload,Tmp_Install_System
RegHiveUnload,Tmp_System

///////////////////////////////////////////////////////////////////////////////////
// Shortcuts

If,%cb_DesktopShc%,Equal,True,AddShortcut,Desktop,,%txt_ShortcutName%,"#$pwindir#$p\Explorer.exe"
If,%cb_StartMenuShc%,Equal,True,AddShortcut,StartMenu,%txt_StartMenuFolder%,%txt_ShortcutName%,"#$pwindir#$p\Explorer.exe"
If,%cb_PinToTaskbar%,Equal,True,PinShortcut,Taskbar,0,"#$pwindir#$p\Explorer.exe"
If,%cb_PinToStartMenu%,Equal,True,PinShortcut,StartMenu,0,"#$pwindir#$p\Explorer.exe"

[#Reg-Themes#]
// ===============================================================================================================================
// Name...........: Reg-Themes
// Description....: Configure Registry settings related to Explorer Themes 
// Syntax.........: 
// Parameters.....: 
// Return values..: 
// Author.........: Homes32
// Remarks........: 
// Related........: Called from [Process]
// ===============================================================================================================================
[Reg-Themes]
Echo,"Registering Themes Support..."

RegImport,%ProjectTemp%\Themes-RegDefault.reg

If,Not,%RegistrySoftwareSource%,Equal,"Install.wim",Begin
  If,ExistRegSubKey,HKLM,"Tmp_Install_Software\Microsoft\Direct3D",RegCopy,HKLM,"Tmp_Install_Software\Microsoft\Direct3D",HKLM,"Tmp_Software\Microsoft\Direct3D"
  RegCopy,HKLM,"Tmp_Install_Software\Microsoft\DirectDraw",HKLM,"Tmp_Software\Microsoft\DirectDraw"
  RegCopy,HKLM,"Tmp_Install_Software\Microsoft\DirectInput",HKLM,"Tmp_Software\Microsoft\DirectInput"
  RegCopy,HKLM,"Tmp_Install_Software\Microsoft\DirectMusic",HKLM,"Tmp_Software\Microsoft\DirectMusic"
  If,ExistRegSubKey,HKLM,"Tmp_Install_Software\Microsoft\DirectPlay",RegCopy,HKLM,"Tmp_Install_Software\Microsoft\DirectPlay",HKLM,"Tmp_Software\Microsoft\DirectPlay"
  RegCopy,HKLM,"Tmp_Install_Software\Microsoft\DirectPlay8",HKLM,"Tmp_Software\Microsoft\DirectPlay8"
  RegCopy,HKLM,"Tmp_Install_Software\Microsoft\DirectPlayNATHelp",HKLM,"Tmp_Software\Microsoft\DirectPlayNATHelp"
  RegCopy,HKLM,"Tmp_Install_Software\Microsoft\DirectShow",HKLM,"Tmp_Software\Microsoft\DirectShow"
  RegCopy,HKLM,"Tmp_Install_Software\Microsoft\DirectX",HKLM,"Tmp_Software\Microsoft\DirectX"

  RegCopy,HKLM,"Tmp_Install_Software\Microsoft\RADAR",HKLM,"Tmp_Software\Microsoft\RADAR"
  RegCopy,HKLM,"Tmp_Install_Software\Microsoft\Windows\CurrentVersion\Control Panel",HKLM,"Tmp_Software\Microsoft\Windows\CurrentVersion\Control Panel"
  RegCopy,HKLM,"Tmp_Install_Software\Microsoft\Windows\CurrentVersion\Controls Folder",HKLM,"Tmp_Software\Microsoft\Windows\CurrentVersion\Controls Folder"
  RegCopy,HKLM,"Tmp_Install_Software\Microsoft\Windows\CurrentVersion\Themes",HKLM,"Tmp_Software\Microsoft\Windows\CurrentVersion\Themes"
  RegCopy,HKLM,"Tmp_Install_Software\Microsoft\Windows\DWM",HKLM,"Tmp_Software\Microsoft\Windows\DWM"
End

RegCopy,HKLM,"Tmp_Install_System\ControlSet001\Services\CoreMessagingRegistrar",HKLM,"Tmp_System\ControlSet001\Services\CoreMessagingRegistrar"
RegCopy,HKLM,"Tmp_Install_System\ControlSet001\Services\Themes",HKLM,"Tmp_System\ControlSet001\Services\Themes"
RegCopy,HKLM,"Tmp_Install_System\ControlSet001\Services\WindowsTrustedRT",HKLM,"Tmp_System\ControlSet001\Services\WindowsTrustedRT"
RegWrite,HKLM,0x4,"Tmp_Software\Microsoft\Windows\DWM","OneCoreNoBootDWM",0
RegWrite,HKLM,0x4,"Tmp_Software\Microsoft\Windows\DWM","ColorPrevalence",1
RegWrite,HKLM,0x0,"Tmp_System\Setup\AllowStart\Themes"
RegWrite,HKLM,0x0,"Tmp_System\Setup\AllowStart\CoreMessagingRegistrar"

[#Reg-Explorer#]
// ===============================================================================================================================
// Name...........: Reg-Explorer
// Description....: Configure Registry settings related to Explorer 
// Syntax.........: 
// Parameters.....: 
// Return values..: 
// Author.........: Homes32
// Remarks........: 
// Related........: Called from [Process]
// ===============================================================================================================================
[Reg-Explorer]
Echo,"Registering Explorer Shell..."
RegImport,%ProjectTemp%\Explorer-RegDefault.reg

// Shell New Context Menu
RegWrite,HKLM,0x7,"Tmp_Default\Software\Microsoft\Windows\CurrentVersion\Explorer\Discardable\PostSetup\ShellNew","Classes",".library-ms",".txt","Folder"
RegWrite,HKLM,0x3,"Tmp_Default\Software\Microsoft\Windows\CurrentVersion\Explorer\Discardable\PostSetup\ShellNew","~reserved~",08,00,00,00,00,00,06,00

If,Not,%RegistrySoftwareSource%,Equal,"Install.wim",Begin
  RegCopy,HKLM,"Tmp_Install_Software\Microsoft\Windows\CurrentVersion\Explorer",HKLM,"Tmp_Software\Microsoft\Windows\CurrentVersion\Explorer"
  RegCopy,HKLM,"Tmp_Install_Software\Microsoft\Windows\CurrentVersion\Policies\Explorer",HKLM,"Tmp_Software\Microsoft\Windows\CurrentVersion\Policies\Explorer"
  RegWrite,HKLM,0x1,"Tmp_Software\Microsoft\Windows NT\CurrentVersion\Winlogon",shell,explorer.exe
  // Registry cached version of the Start menu + Windows.CloudStore.dll are required for Pintool
  RegCopy,HKLM,"Tmp_Install_Software\Microsoft\Windows\CurrentVersion\CloudStore",HKLM,"Tmp_Software\Microsoft\Windows\CurrentVersion\CloudStore"
  RegImport,%ProjectTemp%\Explorer-RegSoftware.reg
  Run,%ScriptFile%,Reg-AdditionalFonts
End

//Cleanup Desktop Context Menu
If,ExistRegSubKey,HKLM,"Tmp_Software\Classes\DesktopBackground\Shell\Display",RegDelete,HKLM,"Tmp_Software\Classes\DesktopBackground\Shell\Display"
If,ExistRegSubKey,HKLM,"Tmp_Software\Classes\DesktopBackground\Shell\Personalize",RegDelete,HKLM,"Tmp_Software\Classes\DesktopBackground\Shell\Personalize"

// Redirect modern Settings (eg. ms-settings:dateandtime) to Control Panel
RegWrite,HKLM,0x2,"Tmp_Software\Classes\ms-settings\Shell\Open\Command","","#$pSystemRoot#$p\system32\control.exe"

// Remove duplicate Removable Drive Icon from Navigation Pane
If,ExistRegSubKey,HKLM,"Tmp_Software\Microsoft\Windows\CurrentVersion\Explorer\Desktop\NameSpace\DelegateFolders\{F5FB2C77-0E2F-4A16-A381-3E560C68BC83}",RegDelete,HKLM,"Tmp_Software\Microsoft\Windows\CurrentVersion\Explorer\Desktop\NameSpace\DelegateFolders\{F5FB2C77-0E2F-4A16-A381-3E560C68BC83}"
