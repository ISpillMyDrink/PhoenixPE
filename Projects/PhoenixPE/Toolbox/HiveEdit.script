[Main]
Title=Hive Editor
Description=Mount and Edit your PE registry hives.
Author=Homes32
Level=10
Selected=None
Mandatory=False
Version=1.0.0.0
Date=2019-04-17

[Process]
Halt,"This script is not designed to be run directly."

[#EditHives#]
// ===============================================================================================================================
// Name...........: EditHives
// Description....: Mount the selected hives, launch regedit.exe and wait for it to terminate. Then unmount the selected hives.
// Syntax.........: 
// Parameters.....: 
// Return values..: 
// Author.........: Homes32
// Remarks........:
// Related........: [UnloadHives]
// ===============================================================================================================================
[EditHives]
Run,%ScriptFile%,LoadHives
Echo,"Regedit is running. Please close Regedit to continue."
ShellExecute,Open,"regedit.exe",""
Run,%ScriptFile%,UnloadHives

[#LoadHives#]
// ===============================================================================================================================
// Name...........: LoadHives
// Description....: Mount the selected hives.
// Syntax.........: 
// Parameters.....: 
// Return values..: 
// Author.........: Homes32
// Remarks........: You must unload the hives manually when you are finished with them.
// Related........: [ForceUnloadHives], [UnloadHives]
// ===============================================================================================================================
[LoadHives]
Echo,"Loading selected registry hives..."
If,%cb_TargetSoftwareHive%,Equal,True,Begin,
  If,Not,ExistFile,%RegSoftware%,Halt,"The Software registry hive was not found."
  RegHiveLoad,Tmp_Software,%RegSoftware%
End
If,%cb_TargetSystemHive%,Equal,True,Begin
  If,Not,ExistFile,%RegSystem%,Halt,"The System registry hive was not found."
  RegHiveLoad,Tmp_System,%RegSystem%
End
If,%cb_TargetDefaultHive%,Equal,True,Begin
  If,Not,ExistFile,%RegDefault%,Halt,"The Default registry hive was not found."
  RegHiveLoad,Tmp_Default,%RegDefault%
End  
If,%cb_TargetDriversHive%,Equal,True,Begin
  If,Not,ExistFile,%RegDrivers%,Halt,"The Drivers registry hive was not found."
  RegHiveLoad,Tmp_Drivers,%RegDrivers%
End

If,%cb_InstallSoftwareHive%,Equal,True,RegHiveLoad,Tmp_Install_Software,%RegInstallSoftware%
If,%cb_InstallSystemHive%,Equal,True,RegHiveLoad,Tmp_Install_System,%RegInstallSystem%
If,%cb_InstallDefaultHive%,Equal,True,RegHiveLoad,Tmp_Install_Default,%RegInstallDefault%
If,%cb_InstallDriversHive%,Equal,True,RegHiveLoad,Tmp_Install_Drivers,%RegInstallDrivers%

If,%cb_CustomSoftwareHive%,Equal,True,RegHiveLoad,Tmp_Custom_Software,"%fb_CustomHives%\SOFTWARE"
If,%cb_CustomSystemHive%,Equal,True,RegHiveLoad,Tmp_Custom_System,"%fb_CustomHives%\SYSTEM"
If,%cb_CustomDefaultHive%,Equal,True,RegHiveLoad,Tmp_Custom_Default,"%fb_CustomHives%\DEFAULT"
If,%cb_CustomDriversHive%,Equal,True,RegHiveLoad,Tmp_Custom_Drivers,"%fb_CustomHives%\DRIVERS"

[#UnloadHives#]
// ===============================================================================================================================
// Name...........: UnloadHives
// Description....: Unload the selected hives.
// Syntax.........: 
// Parameters.....: 
// Return values..: 
// Author.........: Homes32
// Remarks........:
// Related........: [LoadHives]
// ===============================================================================================================================
[UnloadHives]
Echo,"Unloading registry hives..."
If,%cb_TargetSoftwareHive%,Equal,True,RegHiveUnLoad,Tmp_Software
If,%cb_TargetSystemHive%,Equal,True,RegHiveUnLoad,Tmp_System
If,%cb_TargetDefaultHive%,Equal,True,RegHiveUnLoad,Tmp_Default
If,%cb_TargetDriversHive%,Equal,True,RegHiveUnLoad,Tmp_Drivers

If,%cb_InstallSoftwareHive%,Equal,True,RegHiveUnLoad,Tmp_Install_Software
If,%cb_InstallSystemHive%,Equal,True,RegHiveUnLoad,Tmp_Install_System
If,%cb_InstallDefaultHive%,Equal,True,RegHiveUnLoad,Tmp_Install_Default
If,%cb_InstallDriversHive%,Equal,True,RegHiveUnLoad,Tmp_Install_Drivers

If,%cb_CustomSoftwareHive%,Equal,True,RegHiveUnLoad,Tmp_Custom_Software
If,%cb_CustomSystemHive%,Equal,True,RegHiveUnLoad,Tmp_Custom_System
If,%cb_CustomDefaultHive%,Equal,True,RegHiveUnLoad,Tmp_Custom_Default
If,%cb_CustomDriversHive%,Equal,True,RegHiveUnLoad,Tmp_Custom_Drivers

Run,%ScriptFile%,Cleanup-TransactionLogs

[ForceUnloadHives]
// ===============================================================================================================================
// Name...........: ForceUnloadHives
// Description....: Unload ALL registry hives mounted to HKLM regardless of whether or not we mounted them.
// Syntax.........: 
// Parameters.....: 
// Return values..: 
// Author.........: Homes32
// Remarks........:
// Related........: [ForceUnloadHives], [UnloadHives]
// ===============================================================================================================================
[ForceUnloadHives]
Echo,"Unloading all registry hives..."
If,ExistFile,%Tools%\HiveUnload.exe,ShellExecute,Hide,%Tools%\HiveUnload.exe,"/HKLM /Target:ALL"
If,%ExitCode%,Equal,0,Message,"HiveUnload: All registry hives unloaded successfully."
If,%ExitCode%,Equal,1,Message,"HiveUnload: Warning: Not all registry hives could be unloaded! Please verify that regedit is not running and try again.",Warn
If,%ExitCode%,Equal,2,Message,"HiveUnload: No loaded registry hives were found."
Run,%ScriptFile%,Cleanup-TransactionLogs

[#Cleanup-TransactionLogs#]
// ===============================================================================================================================
// Name...........: Cleanup-TransactionLogs
// Description....: Remove transaction logs created by our reg modifications.
// Syntax.........: 
// Parameters.....: 
// Return values..: 
// Author.........: Homes32
// Remarks........:
// Related........: [ForceUnloadHives], [UnloadHives]
// ===============================================================================================================================
[Cleanup-TransactionLogs]
Echo,"Cleaning up transaction logs..."

// Reg Cache
FileDeleteEx,%RegistryCache%\*.LOG1
FileDeleteEx,%RegistryCache%\*.LOG2
FileDeleteEx,%RegistryCache%\*.blf
FileDeleteEx,%RegistryCache%\*.regtrans-ms

// Target
FileDeleteEx,%TargetDir%\Windows\System32\config\*.LOG1
FileDeleteEx,%TargetDir%\Windows\System32\config\*.LOG2
FileDeleteEx,%TargetDir%\Windows\System32\config\*.blf
FileDeleteEx,%TargetDir%\Windows\System32\config\*.regtrans-ms

// Custom
FileDeleteEx,%fb_CustomHives%\Windows\System32\config\*.LOG1
FileDeleteEx,%fb_CustomHives%\Windows\System32\config\*.LOG2
FileDeleteEx,%fb_CustomHives%\Windows\System32\config\*.blf
FileDeleteEx,%fb_CustomHives%\Windows\System32\config\*.regtrans-ms

[#SetDefaultOptions#]
// ===============================================================================================================================
// Name...........: SetDefaultOptions
// Description....: Sets script options to default values.
// Syntax.........: 
// Parameters.....: 
// Return values..: 
// Author.........: Homes32
// Remarks........: 
// Related........: btn_SetDefaultOptions
// ===============================================================================================================================
[SetDefaultOptions]
System,Cursor,Wait
WriteInterface,Value,%ScriptFile%,Interface,cb_TargetSoftwareHive,True
WriteInterface,Value,%ScriptFile%,Interface,cb_TargetSystemHive,True
WriteInterface,Value,%ScriptFile%,Interface,cb_TargetDefaultHive,True
WriteInterface,Value,%ScriptFile%,Interface,cb_TargetDriversHive,False
WriteInterface,Value,%ScriptFile%,Interface,cb_InstallSoftwareHive,False
WriteInterface,Value,%ScriptFile%,Interface,cb_InstallSystemHive,False
WriteInterface,Value,%ScriptFile%,Interface,cb_InstallDefaultHive,False
WriteInterface,Value,%ScriptFile%,Interface,cb_InstallDriversHive,False
WriteInterface,Value,%ScriptFile%,Interface,cb_CustomSoftwareHive,False
WriteInterface,Value,%ScriptFile%,Interface,cb_CustomSystemHive,False
WriteInterface,Value,%ScriptFile%,Interface,cb_CustomDefaultHive,False
WriteInterface,Value,%ScriptFile%,Interface,cb_CustomDriversHive,False
WriteInterface,Value,%ScriptFile%,Interface,fb_CustomHives,""
System,Cursor,Normal

[#ToggleAdvancedOptions#]
// ===============================================================================================================================
// Name...........: ToggleAdvancedOptions
// Description....: Show/Hide Advanced options in the script's GUI
// Syntax.........: 
// Parameters.....: 
// Return values..: 
// Author.........: Homes32
// Remarks........: 
// Related........: Called by [Interface] btn_AdvancedOptions
// ===============================================================================================================================
[ToggleAdvancedOptions]
System,SetLocal
ReadInterface,Visible,%ScriptFile%,Interface,bvl_CustomHives,%state%
If,%state%,Equal,False,Begin,
  If,Question,"WARNING!#$x#$xAdvanced options are intended for expert users only!#$x#$xModifying advanced options without knowing what you are doing can cause permanent damage to your project and may result in unexpected behavior and/or build failures.#$x#$xAre you sure you want to continue?",Set,%Toggle%,True
  Else,Set,%Toggle%,False
End
Else,Set,%Toggle%,False

WriteInterface,Visible,%ScriptFile%,Interface,bvl_InstallHives,%Toggle%
WriteInterface,Visible,%ScriptFile%,Interface,cb_InstallSoftwareHive,%Toggle%
WriteInterface,Visible,%ScriptFile%,Interface,cb_InstallSystemHive,%Toggle%
WriteInterface,Visible,%ScriptFile%,Interface,cb_InstallDefaultHive,%Toggle%
WriteInterface,Visible,%ScriptFile%,Interface,cb_InstallDriversHive,%Toggle%
WriteInterface,Visible,%ScriptFile%,Interface,bvl_CustomHives,%Toggle%
WriteInterface,Visible,%ScriptFile%,Interface,cb_CustomSoftwareHive,%Toggle%
WriteInterface,Visible,%ScriptFile%,Interface,cb_CustomSystemHive,%Toggle%
WriteInterface,Visible,%ScriptFile%,Interface,cb_CustomDefaultHive,%Toggle%
WriteInterface,Visible,%ScriptFile%,Interface,cb_CustomDriversHive,%Toggle%
WriteInterface,Visible,%ScriptFile%,Interface,lbl_CustomHivePath,%Toggle%
WriteInterface,Visible,%ScriptFile%,Interface,fb_CustomHives,%Toggle%
WriteInterface,Visible,%ScriptFile%,Interface,lbl_CustomHiveInfo,%Toggle%
System,EndLocal

[#ShowScriptInfo#]
// ===============================================================================================================================
// Name...........: ShowScriptInfo
// Description....: 
// Syntax.........: 
// Parameters.....: 
// Return values..: 
// Author.........: Homes32
// Remarks........:
// Related........: btn_ScriptInfo
// ===============================================================================================================================
[ShowScriptInfo]
Message,"This script will allow you to mount and edit the registry hives associated with your PE. This can be useful if you need to make manual modifications, compare hives from other projects, or unmount all hives in the case of a build failure."

