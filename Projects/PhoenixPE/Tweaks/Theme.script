// ===============================================================================================================================
[SetDefaultOptions]
System,Cursor,Wait
WriteInterface,Value,%ScriptFile%,Interface,cmb_Theme,"Default"
WriteInterface,Value,%ScriptFile%,Interface,fb_CustomTheme,""
WriteInterface,Value,%ScriptFile%,Interface,cmb_ColorProfile,"Auto"
