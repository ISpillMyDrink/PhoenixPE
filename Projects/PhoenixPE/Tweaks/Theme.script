[Main]
Title=Theme
Author=Homes32
Description=Customize the theme and window colors.
Version=1.0.0.0
Selected=True
Mandatory=False
Date=2019-03-12
Level=4
Interface=Interface

[Variables]

[Process]
Echo,"Processing %ScriptTitle%..."

RegHiveLoad,Tmp_Default,%RegDefault%
RegHiveLoad,Tmp_Software,%RegSoftware%

If,%cb_UXPatch%,Equal,True,Run,%ScriptFile%,UXPatch
Run,%ScriptFile%,ColorMeBadd

RegHiveUnLoad,Tmp_Default
RegHiveUnLoad,Tmp_Software


[#UXPatch#]
// ===============================================================================================================================
// Name...........: UXPatch
// Description....: Patch the necessary .dll files to bypass .msstyles signature check
// Syntax.........: Run,%ScriptFile%,UXPatch
// Parameters.....: 
// Return values..: 
// Author.........: Homes32
// Remarks........: Depends on UXPatch.exe extracted from the script's attachments.
//                  UXPatch.exe (v1.03) is developed by JFX. (http://theoven.org/index.php?topic=2768.0)
// Related........: %cmb_UXPatch%
// ===============================================================================================================================
[UXPatch]
Echo,"Patching theme files..."
ExtractFile,%ScriptFile%,Tools,UxPatch.exe,%ProjectTemp%
ShellExecute,Hide,"%ProjectTemp%\UxPatch.exe","P %TargetSystem32%\themeui.dll %TargetSystem32%\UXInit.dll %TargetSystem32%\uxtheme.dll"
// TODO: Check Return Code

[#ColorMeBadd#]
// ===============================================================================================================================
// Name...........: ColorMeBadd
// Description....: Set the color profile based on the script config. (All For You...)
// Syntax.........: Run,%ScriptFile%,ColorMeBadd
// Parameters.....: 
// Return values..: 
// Author.........: Homes32
// Remarks........:
// Related........:  %cmb_ColorProfile%
// ===============================================================================================================================
[ColorMeBadd]
Echo,"Setting color profile to [%cmb_ColorProfile%]..."

// Configure some basic settings

// Turn of AutoColorization. We will turn this on again later if the user selects Auto.
RegWrite,HKLM,0x4,"Tmp_Default\Control Panel\Desktop",AutoColorization,0
RegWrite,HKLM,0x4,Tmp_Default\Software\Microsoft\Windows\DWM,ColorizationGlassAttribute,0
//RegWrite,HKLM,0x4,Tmp_Default\Software\Microsoft\Windows\DWM,ColorPrevalence,0
RegWrite,HKLM,0x4,Tmp_Default\SOFTWARE\Microsoft\Windows\DWM,Composition,1
//RegWrite,HKLM,0x4,Tmp_Default\Software\Microsoft\Windows\CurrentVersion\Themes\Personalize,ColorPrevalence,0
RegWrite,HKLM,0x4,Tmp_Default\Software\Microsoft\Windows\CurrentVersion\Themes\Personalize,EnableTransparency,1

If,%cmb_ColorProfile%,Equal,"Auto",Begin
  // Auto select a color profile based on the background color/image
  RegWrite,HKLM,0x4,"Tmp_Default\Control Panel\Desktop","AutoColorization",1
  AddVariables,%ScriptFile%,ColorProfile-Default Blue
End
Else,If,%cmb_ColorProfile%,Equal,"Host Color Profile",Begin
  // Use the color profile from the Host OS
  Run,%ScriptFile%,ReadHostColorProfile
End
Else,If,%cmb_ColorProfile%,Equal,"Custom",Begin
  // User defined color profile.
  // No validation is performed. The user is expected to provide valid input or face the consequences.
  Set,%StartColorMenu%,%txt_StartMenuColor%
  Set,%AccentColorMenu%,%txt_AccentColorMenu%
  Set,%AccentColor%,%txt_AccentColor%
  Set,%ColorizationColor%,%txt_ColorizationColor%
  Set,%ColorizationAfterglow%,%txt_ColorizationAfterglow%
  Set,%AccentPalette%,%txt_AccentPalette%
End
Else,Begin
  // Use a color preset
  AddVariables,%ScriptFile%,ColorProfile-%cmb_ColorProfile%
End

// Write our Color Config if we are using Host Profile or Custom
RegWrite,HKLM,0x4,"Tmp_Default\SOFTWARE\Microsoft\Windows\DWM","AccentColor",%AccentColor%
RegWrite,HKLM,0x4,"Tmp_Default\SOFTWARE\Microsoft\Windows\DWM","ColorizationColor",%ColorizationColor%
RegWrite,HKLM,0x4,"Tmp_Default\SOFTWARE\Microsoft\Windows\DWM","ColorizationAfterglow",%ColorizationAfterglow%
RegWrite,HKLM,0x3,"Tmp_Default\SOFTWARE\Microsoft\Windows\CurrentVersion\Explorer\Accent","AccentPalette",%AccentPalette%
RegWrite,HKLM,0x4,"Tmp_Default\SOFTWARE\Microsoft\Windows\CurrentVersion\Explorer\Accent","StartColorMenu",%StartColorMenu%
RegWrite,HKLM,0x4,"Tmp_Default\SOFTWARE\Microsoft\Windows\CurrentVersion\Explorer\Accent","AccentColorMenu",%AccentColorMenu%

[#ImportHostColorProfile#]
// ===============================================================================================================================
// Name...........: ImportHostColorProfile
// Description....: Import the Color Profile used by the local host system and populate the script interface config
// Syntax.........: 
// Parameters.....: 
// Return values..: 
// Author.........: Homes32
// Remarks........:
// Related........: btn_ImportHostColorProfile
// ===============================================================================================================================
[ImportHostColorProfile]
Run,%ScriptFile%,ReadHostColorProfile

WriteInterface,Value,%ScriptFile%,Interface,txt_StartMenuColor,%StartColorMenu%
WriteInterface,Value,%ScriptFile%,Interface,txt_AccentColorMenu,%AccentColorMenu%
WriteInterface,Value,%ScriptFile%,Interface,txt_AccentColor,%AccentColor%
WriteInterface,Value,%ScriptFile%,Interface,txt_ColorizationColor,%ColorizationColor%
WriteInterface,Value,%ScriptFile%,Interface,txt_ColorizationAfterglow,%ColorizationAfterglow%
WriteInterface,Value,%ScriptFile%,Interface,txt_AccentPalette,%AccentPalette%

[#ReadHostColorProfile#]
// ===============================================================================================================================
// Name...........: ReadHostColorProfile
// Description....: Import the Color Profile used by the local host system.
// Syntax.........: 
// Parameters.....: 
// Return values..: 
// Author.........: Homes32
// Remarks........:
// Related........: ImportHostColorProfile, cmb_ColorProfile, btn_ImportHostColorProfile
// ===============================================================================================================================
[ReadHostColorProfile]
RegRead,HKCU,"SOFTWARE\Microsoft\Windows\DWM","AccentColor",%AccentColor%
RegRead,HKCU,"SOFTWARE\Microsoft\Windows\DWM","ColorizationColor",%ColorizationColor%
RegRead,HKCU,"SOFTWARE\Microsoft\Windows\DWM","ColorizationAfterglow",%ColorizationAfterglow%
RegRead,HKCU,"SOFTWARE\Microsoft\Windows\CurrentVersion\Explorer\Accent","AccentPalette",%AccentPalette%
RegRead,HKCU,"SOFTWARE\Microsoft\Windows\CurrentVersion\Explorer\Accent","StartColorMenu",%StartColorMenu%
RegRead,HKCU,"SOFTWARE\Microsoft\Windows\CurrentVersion\Explorer\Accent","AccentColorMenu",%AccentColorMenu%

